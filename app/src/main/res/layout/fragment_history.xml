<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="15dp"

    tools:context="deltahacks.com.intheloop.categoryHistoryPage"
    android:id="@+id/mainBody">

    ]
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/logoHeader"
        android:src="@drawable/logo"
        android:baselineAlignBottom="false"
        android:scaleType="fitCenter"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="40dp"
        android:paddingBottom="40dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Entertainment"
        android:id="@+id/categoryTitle"
        android:paddingLeft="50dp"

        />

    <GridLayout
        android:paddingLeft="10dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center_horizontal"
        android:rowCount="11"
        android:columnCount="6"
        android:orientation="vertical"
        android:id="@+id/mainGrid">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingRight="20dp"
            android:paddingBottom="40dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text= "temp text 1: date method"
            android:id="@+id/date1"
            android:layout_row="1"
            android:layout_column="1" />

        <VideoView
            android:layout_width="76dp"
            android:layout_height="40dp"
            android:id="@+id/videoView1"
            android:layout_row="1"
            android:layout_column="2" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingBottom="40dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text= "temp text 2: date method"
            android:id="@+id/date2"
            android:layout_row="2"
            android:layout_column="1" />

        <VideoView
            android:layout_width="76dp"
            android:layout_height="40dp"
            android:id="@+id/videoView2"
            android:layout_row="2"
            android:layout_column="2" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingBottom="40dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text= "temp text 3: date method"
            android:id="@+id/date3"
            android:layout_row="3"
            android:layout_column="1" />

        <VideoView
            android:layout_width="76dp"
            android:layout_height="40dp"
            android:id="@+id/videoView3"
            android:layout_row="3"
            android:layout_column="2" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingBottom="40dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text= "temp text 4: date method"
            android:id="@+id/date4"
            android:layout_row="4"
            android:layout_column="1" />

        <VideoView
            android:layout_width="76dp"
            android:layout_height="40dp"
            android:id="@+id/videoView4"
            android:layout_row="4"
            android:layout_column="2" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingBottom="40dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text= "temp text 5: date method"
            android:id="@+id/date5"
            android:layout_row="5"
            android:layout_column="1" />

        <VideoView
            android:layout_width="76dp"
            android:layout_height="40dp"
            android:id="@+id/videoView5"
            android:layout_row="5"
            android:layout_column="2" />

    </GridLayout>


    ]


</LinearLayout>
